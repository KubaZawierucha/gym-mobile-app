<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MusclesActivities.MuscleGroupListActivity">

    <TextView
        android:id="@+id/activity_muscle_group_list_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/TitleText"
        android:text="Select Muscle Group"/>

    <ExpandableListView
        android:layout_marginTop="20dp"
        android:layout_below="@+id/activity_muscle_group_list_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/muscle_group_lv"/>



</RelativeLayout>